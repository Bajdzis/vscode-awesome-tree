{
	"name": "awesome-tree",
	"displayName": "Awesome tree",
	"description": "Stop creating folders, start creating structures! (Automated file creation)",
	"version": "1.4.0",
	"publisher": "bajdzis",
	"author": {
		"name": "RafaÅ‚ Budzis",
		"url": "https://budzis.pl"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"icon": "readme/awesome-tree-icon.png",
	"keywords": [
		"file",
		"files",
		"dir",
		"directory",
		"directories",
		"tree",
		"dirtree",
		"filetree"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Bajdzis/vscode-awesome-tree.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.saveAsTemplate",
				"title": "Save file as awesome template",
				"category": "FILE",
				"icon": {
					"dark": "icons/awesome-template.svg",
					"light": "icons/awesome-template-light.svg"
				}
			},
			{
				"command": "extension.renameDirectory",
				"title": "Rename with edit files [Awesome tree]",
				"category": "FILE",
				"icon": {
					"dark": "icons/awesome-template.svg",
					"light": "icons/awesome-template-light.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.saveAsTemplate",
					"group": "navigation@1",
					"when": "resourceScheme == file"
				}
			],
			"explorer/context": [
				{
					"command": "extension.renameDirectory",
					"group": "7_modification",
					"when": "explorerResourceIsFolder"
				}
			]
		},
		"configuration": {
			"title": "Awesome Tree",
			"properties": {
				"awesomeTree.excludeWatchRegExp": {
					"type": "string",
					"default": "bower_components|node_modules|(\\.git[\\\\|\\/])|\\.svn|\\.hg|\\.DS_Store",
					"description": "Regular expressions for exclude watch files.",
					"deprecationMessage": "Deprecated: Please use awesomeTree.ignorePathsGlob instead."
				},
				"awesomeTree.excludeByGitIgnoreFile": {
					"type": "boolean",
					"default": true,
					"description": "When set to true extension exclude watch files by .gitingore file content"
				},
				"awesomeTree.ignorePathsGlob": {
					"type": "array",
					"description": "Array globs with exclude path.",
					"default": [
						"bower_components/**",
						"node_modules/**",
						".git/**",
						".svn",
						".hg",
						".DS_Store"
					]
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack:profile": "webpack --mode production --profile --json > stats.json",
		"compile": "webpack --mode development",
		"watch": "webpack --mode development",
		"test": "jest",
		"test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
		"lint": "eslint ./ --ext .ts ",
		"prerelease": "standard-version",
		"release": "git push --follow-tags origin master"
	},
	"pre-commit": [
		"lint",
		"test"
	],
	"devDependencies": {
		"@types/glob": "^7.1.2",
		"@types/jest": "^26.0.0",
		"@types/lodash": "^4.14.155",
		"@types/node": "^10.17.26",
		"@types/uuid": "^8.0.0",
		"@types/vscode": "1.37.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"coveralls": "^3.1.0",
		"cross-env": "^7.0.2",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"jest": "^26.0.1",
		"pre-commit": "^1.2.2",
		"standard-version": "^7.0.1",
		"ts-jest": "^26.1.0",
		"typescript": "^3.9.5",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@types/minimatch": "^3.0.3",
		"@types/parse-gitignore": "^1.0.0",
		"lodash": "^4.17.15",
		"minimatch": "^3.0.4",
		"parse-gitignore": "^1.0.1",
		"redux": "^4.0.5",
		"redux-observable": "^1.2.0",
		"rxjs": "^6.5.5",
		"ts-loader": "^7.0.5",
		"typescript-fsa": "^3.0.0",
		"typescript-fsa-reducers": "^1.2.1",
		"uuid": "^8.1.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	}
}

{
	"name": "awesome-tree",
	"displayName": "Awesome tree",
	"description": "Stop creating folders, start creating structures! (Automated file creation)",
	"version": "0.0.13",
	"publisher": "bajdzis",
	"author": {
		"name": "Rafa≈Ç Budzis",
		"url": "https://budzis.pl"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"icon": "readme/awesome-tree-icon.png",
	"keywords": [
		"file",
		"files",
		"dir",
		"directory",
		"directories",
		"tree",
		"dirtree",
		"filetree"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Bajdzis/vscode-awesome-tree.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.saveAsTemplate",
				"title": "Save file as awesome template",
				"category": "FILE",
				"icon": {
					"dark": "icons/awesome-template.svg",
					"light": "icons/awesome-template.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.saveAsTemplate",
					"group": "navigation@1",
					"when": "resourceScheme == file"
				}
			]
		},
		"configuration": {
			"title": "Awesome Tree",
			"properties": {
				"awesomeTree.excludeWatchRegExp": {
					"type": "string",
					"default": "bower_components|node_modules|\\.git|\\.svn|\\.hg|\\.DS_Store",
					"description": "Regular expressions for exclude watch files."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "jest",
		"test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
		"lint": "eslint ./ --ext .ts ",
		"prerelease": "standard-version",
		"release": "git push --follow-tags origin master"
	},
	"pre-commit": [
		"lint",
		"test"
	],
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jest": "^24.0.18",
		"@types/lodash": "^4.14.138",
		"@types/node": "^10.12.21",
		"@types/uuid": "^3.4.5",
		"@types/vscode": "^1.38.0",
		"@typescript-eslint/eslint-plugin": "^2.3.0",
		"@typescript-eslint/parser": "^2.3.0",
		"coveralls": "^3.0.6",
		"eslint": "^6.4.0",
		"glob": "^7.1.4",
		"jest": "^24.9.0",
		"pre-commit": "^1.2.2",
		"standard-version": "^7.0.0",
		"ts-jest": "^24.1.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"lodash": "^4.17.15",
		"uuid": "^3.3.3"
	}
}
